{
  "name": "setbased/php-audit",
  "description": "A tool for creating audit tables and triggers for logging data changes in a MySQL database.",
  "keywords": [
    "MySQL",
    "Audit",
    "Triggers"
  ],
  "type": "tool",
  "license": "MIT",
  "require": {
    "php": ">=7.0.0",
    "setbased/exception": "^1.0.0",
    "setbased/helper-code-store-mysql": "^1.0.0",
    "setbased/php-stratum": "^0.9.0",
    "symfony/console": "^3.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0.0",
    "phing/phing": "^2.0.0"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Audit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Audit\\Test\\": "test/"
    }
  },
  "bin": [
    "bin/audit"
  ],
  "config": {
    "bin-dir": "bin"
  }
}
