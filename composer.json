{
  "name": "setbased/php-audit",
  "description": "A tool for creating audit tables and triggers for logging data changes in a MySQL database.",
  "keywords": [
    "MySQL",
    "audit",
    "triggers"
  ],
  "type": "tool",
  "license": "MIT",
  "require": {
    "php": ">=5.4.0",
    "setbased/exception": "1.*",
    "setbased/helper-code-store-mysql": "1.*",
    "setbased/php-stratum": "*",
    "symfony/console": "^2.8 || ^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.* || 5.*",
    "phing/phing": "2.*",
    "symfony/process": "*"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Audit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Audit\\Test\\": "test/"
    }
  },
  "bin": [
    "bin/audit"
  ],
  "config": {
    "bin-dir": "bin"
  }
}
